/*------------------------------------------------------------------------*/
/*  event table
/*------------------------------------------------------------------------*/

table.event, table.event th,
div.table.event, div.table.event div.th {
  background: #fff;
}

table.event tr,
div.table.event div.tr {
	background: #d7ffd7;
}
table.event tr:nth-child(even),
div.table.event div.tr:nth-child(even) {
  background: #eeffee;
}

table.event tr.information,
div.table.event div.tr.information {
  background: #fff;
}
table.event tr.information:nth-child(even),
div.table.event div.tr.information:nth-child(even) {
  background: #eee;
}

table.event tr.expired,
div.table.event div.tr.expired {
  background: #ffffd0;
}
table.event tr.expired:nth-child(even),
div.table.event div.tr.expired:nth-child(even) {
  background: #ffffe0;
}

table.event tr.high_priority,
div.table.event div.tr.high_priority {
  background: #ffd0d0;
}
table.event tr.high_priority:nth-child(even),
div.table.event div.tr.high_priority:nth-child(even) {
  background: #ffe0e0;
}

table.event tr.finished,
div.table.event div.tr.finished {
  background: #d0d0ff;
}
table.event tr.finished:nth-child(even),
div.table.event div.tr.finished:nth-child(even) {
  background: #e0e0ff;
}

table.event tr td:first-child { max-width: 100px; }
div.table.event div.tr div.td:first-child { max-width: 100px; }
.event label { display: none; }
div.table div.tr div.td { padding: 6px; }


